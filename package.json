{
    "name": "the-anime-scripter",
    "version": "2.2.10",
    "scripts": {
        "dev": "vite",
        "watch": "tsc && vite build --watch true",
        "build": "npm run clean && tsc -p \"tsconfig-build.json\" && vite build --watch false",
        "zxp": "npm run clean && tsc -p \"tsconfig-build.json\" && cross-env ZXP_PACKAGE=true vite build --watch false",
        "zip": "npm run clean && tsc -p \"tsconfig-build.json\" && cross-env ZIP_PACKAGE=true vite build --watch false",
        "clean": "rimraf dist/* || exit 0",
        "clean-force": "powershell -Command \"if (Test-Path 'dist') { Get-ChildItem -Path 'dist' -Recurse | Remove-Item -Force -Recurse -ErrorAction SilentlyContinue; Start-Sleep 1 }\"",
        "serve": "cross-env SERVE_PANEL=true vite preview",
        "symlink": "cross-env ACTION=symlink vite",
        "delsymlink": "cross-env ACTION=delsymlink vite"
    },
    "dependencies": {
        "@adobe/react-spectrum": "^3.38.0",
        "@react-spectrum/toast": "^3.0.0-beta.17",
        "install": "^0.13.0",
        "jsxbin": "^2.3.0",
        "motion": "^12.18.1",
        "npm": "^10.8.1",
        "react": "^18.2.0",
        "react-copy-to-clipboard": "^5.1.0",
        "react-dom": "^18.2.0",
        "react-icons": "^5.3.0",
        "rollup-plugin-obfuscator": "^1.1.0",
        "socket.io-client": "^4.8.1",
        "uuid": "^11.1.0"
    },
    "devDependencies": {
        "@babel/core": "^7.19.0",
        "@babel/plugin-proposal-class-properties": "^7.16.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.16.0",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.16.4",
        "@babel/preset-env": "^7.19.0",
        "@babel/preset-react": "^7.16.0",
        "@babel/preset-typescript": "^7.16.0",
        "@babel/runtime": "^7.16.3",
        "@rollup/plugin-babel": "^5.3.1",
        "@rollup/plugin-commonjs": "^21.0.2",
        "@rollup/plugin-image": "^2.1.1",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^13.1.3",
        "@rollup/plugin-replace": "^4.0.0",
        "@types/fs-extra": "^9.0.13",
        "@types/node": "^16.11.7",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.0.8",
        "@types/trusted-types": "^2.0.2",
        "@vitejs/plugin-react": "^3.0.1",
        "babel-plugin-react-require": "^3.1.3",
        "babel-plugin-transform-scss": "^1.1.0",
        "babel-preset-env": "^1.7.0",
        "core-js": "3",
        "cross-env": "^7.0.3",
        "fs-extra": "^10.0.0",
        "javascript-obfuscator": "^4.1.1",
        "prettier": "^3.5.3",
        "rimraf": "^3.0.2",
        "rollup": "^2.68.0",
        "rollup-plugin-multi-input": "^1.3.1",
        "rollup-plugin-node-copy": "^1.0.4",
        "rollup-plugin-scss": "^3.0.0",
        "sass": "^1.43.4",
        "terser": "^5.43.1",
        "types-for-adobe": "^7.2.0",
        "types-for-adobe-extras": "^0.0.6",
        "typescript": "^4.6.4",
        "vite": "^4.5.2",
        "vite-cep-plugin": "^1.2.9",
        "vite-plugin-javascript-obfuscator": "^3.1.0",
        "yarn": "^1.22.17"
    },
    "main": "index.js",
    "license": "MIT"
}
